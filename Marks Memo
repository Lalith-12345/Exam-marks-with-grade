#include <stdio.h>
main()
{
  printf("enter your marks :");
  int marks=0;
  scanf("%d",&marks);
  
  if(marks<40)
  {
    printf("Grade F");
  }
  if(39<marks && marks<55)
  {
    printf("Grade D");
  }
  if(54<marks && marks<70)
  {
    printf("Grade C");
  }
  if(69<marks && marks<85)
  {
    printf("Grade B");
  }
  if(84<marks && marks<101)
  {
    printf("Grade A");
  }
  return 0;
}
